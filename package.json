  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "client": "npx expo start --web",
    "server": "cd functions && netlify functions:serve --functions ./ -p 8888",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "npx expo export:web && netlify functions:build -f web-build/functions --src functions && netlify deploy --dir web-build",
    "deploy": "netlify deploy --prod -d web-build",
    "postinstall": "patch-package"
  },
